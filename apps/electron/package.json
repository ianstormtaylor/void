{
  "name": "@apps/electron",
  "productName": "Void",
  "private": true,
  "version": "0.0.2",
  "description": "A visual studio for making generative art.",
  "author": "Ian Storm Taylor",
  "license": "MIT",
  "repository": "git://github.com/ianstormtaylor/void.git",
  "main": "./out/main/index.js",
  "scripts": {
    "build": "electron-vite build && dotenv -- electron-builder --config ./electron-builder.config.js --mac --win --linux",
    "build:preview": "electron-vite build --mode preview && dotenv -- electron-builder --mac -c.mac.identity=null",
    "debug": "./dist/mac/Void.app/Contents/MacOS/Void --remote-debugging-port=8315 --enable-logging",
    "debug:release": "/Applications/Void.app/Contents/MacOS/Void --remote-debugging-port=8315 --enable-logging",
    "dev": "electron-vite dev",
    "dev:preview": "electron-vite preview --mode preview",
    "logs": "code ~/Library/Logs/Void/main.log",
    "release": "npm run build -- --publish always"
  },
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "dotenv-cli": "^6.0.0",
    "electron-devtools-vendor": "^1.1.0",
    "electron-notarize": "^1.2.1",
    "electron-vite": "^1.0.13",
    "esbuild-wasm": "^0.15.16",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^1.3.2",
    "change-case": "^4.1.2",
    "electron": "19.0.3",
    "electron-builder": "^23.0.3",
    "electron-log": "^4.4.8",
    "electron-store": "^8.0.1",
    "electron-unhandled": "^4.0.1",
    "fix-path": "^4.0.0",
    "immer": "^9.0.15",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "platform": "1.3.6",
    "plur": "^5.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.4.0",
    "react-merge-refs": "^2.0.1",
    "react-router-dom": "^6.4.0",
    "react-use": "^17.4.0",
    "source-map": "^0.5.7",
    "stacktrace-parser": "0.1.10",
    "svg2pdf.js": "^2.2.0",
    "svgcanvas": "^2.3.0",
    "tempy": "^3.0.0",
    "update-electron-app": "^2.0.1",
    "use-immer": "^0.7.0",
    "vite": "*",
    "void": "*"
  }
}
