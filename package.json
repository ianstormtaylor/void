{
  "private": true,
  "name": "void-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "git://github.com/ianstormtaylor/void.git",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv -- npm run build --workspace=void && dotenv -- npm run build --workspace=void-electron",
    "build:prod": "NODE_ENV=production dotenv -- npm run build:prod --workspace=void-electron",
    "clean": "rm -rf ./node_modules && rm -rf ./{apps,packages}/*/{build,dist,node_modules,release}",
    "dev": "dotenv -- npm run dev --workspace=void-electron",
    "open": "open ./apps/electron/release/0.0.0/mac/Void.app",
    "pkg": "dotenv -- npm run pkg --workspace=void-electron"
  },
  "devDependencies": {
    "dotenv-cli": "^6.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3",
    "vite": "^3.1.8",
    "void": "*"
  }
}
