{
  "private": true,
  "name": "void-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "git://github.com/ianstormtaylor/void.git",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build:pkgs && npm run build:apps",
    "build:apps": "npm run build --workspace=@apps/electron",
    "build:pkgs": "npm run build --workspace=void",
    "clean": "rm -rf ./node_modules && rm -rf ./{apps,packages}/*/{dist,node_modules,out}",
    "dev": "npm run dev --workspace=@apps/electron",
    "preview": "npm run preview --workspace=@apps/electron"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "typescript": "^4.7.3",
    "vite": "^3.1.8"
  }
}
